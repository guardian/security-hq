{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "diff": "cdk diff --path-metadata false --version-reporting false",
    "synth": "cdk synth --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/cdk": "50.10.13",
    "@guardian/eslint-config-typescript": "^7.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "20.6.0",
    "aws-cdk": "2.90.0",
    "aws-cdk-lib": "2.90.0",
    "constructs": "10.2.69",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  }
}
