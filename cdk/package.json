{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "test": "jest",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "diff": "cdk diff --path-metadata false --version-reporting false",
    "synth": "cdk synth --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/cdk": "53.0.0",
    "@guardian/eslint-config-typescript": "^9.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.6",
    "aws-cdk": "2.114.1",
    "aws-cdk-lib": "2.114.1",
    "constructs": "10.3.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
